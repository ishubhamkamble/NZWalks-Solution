@model IEnumerable<NZWalksUI.Models.DTO.RegionsDto>
@{
}

<h1 class="mt-3">Regions</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Code</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var region in Model)
        {
            <tr>
                <td>@region.Code</td>
                <td>@region.Name</td>
            </tr>
        }
    </tbody>
</table>
